{% extends 'base.html.twig' %}

{% block body %}
    {% for error in errors %}
        <p><b>Error: </b>{{ error }}</p>
    {% endfor %}

    <form method="post" enctype="multipart/form-data">
        <label for="FirstCompetitionScores">Upload hier het csv bestand met de scores van de eerste
            plaatsingswedstrijd</label>
        <input type="file" name="FirstCompetitionScores" id="FirstCompetitionScores" required>
        <br/><br/>
        <label for="SecondCompetitionScores">Upload hier het csv bestand met de scores van de tweede
            plaatsingswedstrijd</label>
        <input type="file" name="SecondCompetitionScores" id="SecondCompetitionScores" required>
        <br/><br/>
        <input type="submit" value="Upload sheets" name="submit">
    </form>
{% endblock %}
